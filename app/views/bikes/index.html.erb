<div class="card-bike-index-main">
  <h1>All bikes</h1>
  <p> <%= link_to "My published bikes", my_bikes_path %> </p>
  <div class="card-bike-index-cards">
    <% @bikes.each do |bike| %>
      <div class="card-bike-index-card">
        <% if bike.photo.present? %>
          <%= cl_image_tag bike.photo.key %>
        <% end %>
        <div class="bike-info">
          <%= link_to bike.name.capitalize(), new_bike_booking_path(bike) %>  <!--bike_path(bike)-->
          <p><%= bike.price %>.-/day</p>
        </div>
      </div>
    <% end %>
  </div>
</div>
<p> <%= link_to "Back to all bikes", bikes_path %> </p>
