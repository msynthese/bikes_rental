<div class="container" class = "project">
  <%= simple_form_for (bike) do |f| %>
    <%= f.input :name %>
    <%= f.input :category, collection: Bike.categories.keys %>
    <%= f.input :size, collection: Bike.sizes.keys %>
    <%= f.input :price %>
    <%= f.input :location %>
    <%= f.input :picture_url %>
    <%= f.input :description %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>
